if (confirmation) {
    doAction = confirm("Really activate the module? " + nicoh5Url);
} else {
    doAction = true;
}

if(doAction &&  /www\.nicovideo\.jp\/watch\/([^/]+)$/.test(location.href)) {
    window.NicoHTML5_OverlayType     = "dom";
    window.NicoHTML5_CommentInterval = commentInterval;
    window.NicoHTML5_EnableXHttp     = true;

    var e = document.createElement("script");
    e.src = nicoh5Url;
    e.type = "text/javascript";
    document.body.appendChild(e);
}
